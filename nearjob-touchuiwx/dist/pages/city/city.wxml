<view class="ui-dialog-content">
  <view class="ui-dialog-body">
    <ui-index-list height="{{wx.DEFAULT_CONTENT_HEIGHT}}" show-top data="{{list}}" _pid="{{_pid}}">
      <view class="top_box">
        <view>
          <view class="title">坐标定位</view>
          <view class="position">
            <span type="home-map" color="#00ab48" class="ui-icon icon-home-map" style="color: #00ab48;font-size: 16px;"></span>
            {{position}}
          </view>
        </view>
        <view class="top2">
          <view class="title" style="margin-left:-10px">热门城市</view>
          <ui-tags data="{{tagsData}}" bindchange="singleTap" fixed-width col-number="4" _pid="{{_pid}}">
          </ui-tags>
        </view>
      </view>
    </ui-index-list>
  </view>
</view>