.nav-container {
  position: relative;
}
.distanceView {
  padding: 0 10px;
  margin-top: 10px;
  background-color: #fafafa;
  border-top: 1px solid #eee;
}
.distanceBtn {
  margin: 15px;
  color: #fff;
  background-color: #000;
}
.delNotInsJob {
  width: 80px;
  text-align: center;
  line-height: 80px;
  font-size: 14px;
  color: #fff;
  background: #dfdfdf;
}
.top_tip {
  background-color: #fff;
}
.top_tip .left_icon {
  width: 50px;
  height: 50px;
  border-radius: 50%;
}
.top_tip .text text {
  font-size: 12px;
  color: #313338;
}
.top_tip .text view {
  color: #9c9fa4;
  font-size: 10px;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}
